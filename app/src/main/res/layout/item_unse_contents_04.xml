<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:background="@color/color_CCCCCC"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/ll_top_contents"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/iv_title_image"
                    android:layout_width="46dp"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    tools:src="@drawable/gf_01_star1" />

                <TextView
                    android:id="@+id/tv_unse_type_top"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="9dp"
                    android:fontFamily="@font/custom_font_hanb"
                    android:includeFontPadding="false"
                    android:textColor="@color/color_a36743"
                    android:textSize="24dp"
                    tools:text="오늘의운세" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="11dp"
                android:layout_marginTop="21dp"
                android:layout_marginBottom="13dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="13dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="1dp"
                    android:adjustViewBounds="true"
                    android:src="@drawable/gf_beak" />

                <TextView
                    android:id="@+id/tv_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:maxLength="5"
                    android:layout_marginLeft="3dp"
                    android:textColor="@color/color_2c2828"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    tools:text="홍길동" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="님의 "
                    android:textColor="@color/color_2c2828"
                    android:textSize="17dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv_unse_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_2c2828"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    tools:text="오늘의운세" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="입니다."
                    android:textColor="@color/color_2c2828"
                    android:textSize="17dp"
                    android:textStyle="bold" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_contents"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/gf_bgbox1"
            android:layout_marginBottom="11dp"
            android:orientation="vertical"
            android:paddingLeft="22dp"
            android:paddingTop="19dp"
            android:paddingRight="22dp"
            android:paddingBottom="20dp">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:adjustViewBounds="true"
                    android:src="@drawable/gf_talkballoon" />

                <TextView
                    android:id="@+id/today_title"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:layout_weight="1"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textColor="@color/color_3f2520"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    tools:text="오늘의 총 운세" />

                <TextView
                    android:id="@+id/tv_star_count"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:includeFontPadding="false"
                    android:textColor="@color/color_2c2828"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    tools:text="5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"

                    android:layout_marginRight="6dp"
                    android:gravity="bottom"
                    android:text="/5"
                    android:textColor="@color/color_807474"
                    android:textSize="13dp" />

                <ImageView
                    android:id="@+id/today_total_staricon1"
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:src="@drawable/gf_scorestar0" />

                <ImageView
                    android:id="@+id/today_total_staricon2"
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:layout_marginLeft="2dp"
                    android:src="@drawable/gf_scorestar0" />

                <ImageView
                    android:id="@+id/today_total_staricon3"
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:layout_marginLeft="2dp"
                    android:src="@drawable/gf_scorestar0" />

                <ImageView
                    android:id="@+id/today_total_staricon4"
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:layout_marginLeft="2dp"
                    android:src="@drawable/gf_scorestar0" />

                <ImageView
                    android:id="@+id/today_total_staricon5"
                    android:layout_width="17dp"
                    android:layout_height="17dp"
                    android:layout_marginLeft="2dp"
                    android:src="@drawable/gf_scorestar0" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_constellation"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingTop="15dp"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/iv_constellation_left"
                        android:layout_width="82dp"
                        android:layout_height="82dp" />

                    <TextView
                        android:id="@+id/tv_constellation_left"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textColor="@color/color_404040"
                        android:textSize="13dp" />
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ㆍㆍ"
                    android:textColor="@color/color_404040"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/tv_constellation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="@color/color_404040"
                    android:textSize="25dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ㆍㆍ"
                    android:textColor="@color/color_404040"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingTop="15dp"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/iv_constellation_right"
                        android:layout_width="82dp"
                        android:layout_height="82dp" />

                    <TextView
                        android:id="@+id/tv_constellation_right"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textColor="@color/color_404040"
                        android:textSize="13dp" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:id="@+id/today_total_fortune"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:lineSpacingExtra="4dp"
                android:text="금전은 수입이 많은 대인데 그만큼 지출도 "
                android:textColor="@color/color_807474"
                android:textSize="15dp" />

        </LinearLayout>


