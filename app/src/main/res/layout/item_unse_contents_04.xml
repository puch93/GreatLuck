        <LinearLayout
            android:id="@+id/ll_bottom_charm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="9dp"
            android:orientation="vertical">

            <FrameLayout
                android:id="@+id/fl_subscription"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:src="@drawable/gf_morebutton" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="광고 제거하기"
                    android:textColor="@color/white"
                    android:textSize="19dp"
                    android:textStyle="bold" />
            </FrameLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="7dp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingLeft="5dp">

                <ImageView
                    android:layout_width="13dp"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:src="@drawable/gf_beak" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:text="나를 위한 행운의 부적 리스트"
                    android:textColor="@color/color_2c2828"
                    android:textSize="17dp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="13dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/ll_charm1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="9.3dp"
                    android:layout_weight="1"
                    android:background="@drawable/gf_talismanbox"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="10dp">

                    <TextView
                        android:id="@+id/tv_charm_name1"
                        android:layout_width="wrap_content"
                        android:layout_height="33dp"
                        android:layout_marginLeft="1dp"
                        android:fontFamily="@font/custom_font_yi"
                        android:gravity="center"
                        android:lineSpacingExtra="1dp"
                        android:lines="2"
                        android:textColor="@color/color_3f2520"
                        android:textSize="15dp"
                        tools:text="귀인부적" />


                    <ImageView
                        android:id="@+id/iv_charm1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="7dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginRight="6dp"
                        android:adjustViewBounds="true" />


                    <FrameLayout
                        android:id="@+id/fl_ok1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="9dp"
                        android:layout_marginTop="6dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginBottom="8dp">

                        <com.facebook.shimmer.ShimmerFrameLayout
                            android:id="@+id/ll_shimmer1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:shimmer_base_alpha="1"
                            app:shimmer_base_color="@color/white"
                            app:shimmer_colored="false"
                            app:shimmer_duration="2500"
                            app:shimmer_highlight_alpha="0.1">
                            <!-- app:shimmer_animation_duration="1500"
                            app:shimmer_color="@color/white"-->

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:background="@drawable/gf_buybutton_g" />

                        </com.facebook.shimmer.ShimmerFrameLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/custom_font_bmeu"
                            android:text="구매하기"
                            android:textColor="@color/white"
                            android:textSize="15dp" />
                    </FrameLayout>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ll_charm2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4.6dp"
                    android:layout_marginRight="4.6dp"
                    android:layout_weight="1"
                    android:background="@drawable/gf_talismanbox"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="10dp">

                    <TextView
                        android:id="@+id/tv_charm_name2"
                        android:layout_width="wrap_content"
                        android:layout_height="33dp"
                        android:layout_marginLeft="1dp"
                        android:fontFamily="@font/custom_font_yi"
                        android:gravity="center"
                        android:lineSpacingExtra="1dp"
                        android:lines="2"
                        android:textColor="@color/color_3f2520"
                        android:textSize="15dp"
                        tools:text="귀인부적" />


                    <ImageView
                        android:id="@+id/iv_charm2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="7dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginRight="6dp"
                        android:adjustViewBounds="true" />


                    <FrameLayout
                        android:id="@+id/fl_ok2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="9dp"
                        android:layout_marginTop="6dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginBottom="8dp">

                        <com.facebook.shimmer.ShimmerFrameLayout
                            android:id="@+id/ll_shimmer2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:shimmer_base_alpha="1"
                            app:shimmer_base_color="@color/white"
                            app:shimmer_colored="false"
                            app:shimmer_duration="2500"
                            app:shimmer_highlight_alpha="0.1">
                            <!-- app:shimmer_animation_duration="1500"
                            app:shimmer_color="@color/white"-->

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:background="@drawable/gf_buybutton_g" />

                        </com.facebook.shimmer.ShimmerFrameLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/custom_font_bmeu"
                            android:text="구매하기"
                            android:textColor="@color/white"
                            android:textSize="15dp" />
                    </FrameLayout>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ll_charm3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="9.3dp"
                    android:layout_weight="1"
                    android:background="@drawable/gf_talismanbox"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="10dp">

                    <TextView
                        android:id="@+id/tv_charm_name3"
                        android:layout_width="wrap_content"
                        android:layout_height="33dp"
                        android:layout_marginLeft="1dp"
                        android:fontFamily="@font/custom_font_yi"
                        android:gravity="center"
                        android:lineSpacingExtra="1dp"
                        android:lines="2"
                        android:textColor="@color/color_3f2520"
                        android:textSize="15dp"
                        tools:text="귀인부적" />


                    <ImageView
                        android:id="@+id/iv_charm3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="7dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginRight="6dp"
                        android:adjustViewBounds="true" />

                    <FrameLayout
                        android:id="@+id/fl_ok3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="9dp"
                        android:layout_marginTop="6dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginBottom="8dp">

                        <com.facebook.shimmer.ShimmerFrameLayout
                            android:id="@+id/ll_shimmer3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:shimmer_base_alpha="1"
                            app:shimmer_base_color="@color/white"
                            app:shimmer_colored="false"
                            app:shimmer_duration="2500"
                            app:shimmer_highlight_alpha="0.1">
                            <!-- app:shimmer_animation_duration="1500"
                            app:shimmer_color="@color/white"-->

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:background="@drawable/gf_buybutton_g" />

                        </com.facebook.shimmer.ShimmerFrameLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/custom_font_bmeu"
                            android:text="구매하기"
                            android:textColor="@color/white"
                            android:textSize="15dp" />
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>

            <FrameLayout
                android:id="@+id/fl_more_charm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:src="@drawable/gf_morebutton" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="더 많은 부적 보기"
                    android:textColor="@color/white"
                    android:textSize="19dp"
                    android:textStyle="bold" />
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>